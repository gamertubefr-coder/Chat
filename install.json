{
  "run": [
    {
      "method": "shell.run",
      "params": {
        "message": [
          "git clone https://github.com/SUP3RMASS1VE/Qwen3-TTS app"
        ]
      }
    },
    {
      "method": "fs.copy",
      "params": {
        "src": "app.py",
        "dest": "app/app.py"
      }
    },
    {
      "method": "fs.copy",
      "params": {
        "src": "app_mac.py",
        "dest": "app/app_mac.py"
      }
    },
    {
      "method": "shell.run",
      "params": {
        "path": "app",
        "message": [
          "git clone --recursive https://github.com/FunAudioLLM/CosyVoice.git CosyVoice"
        ]
      }
    },
    {
      "method": "shell.run",
      "params": {
        "message": [
          "conda install -y sox"
        ]
      }
    },
    {
      "method": "shell.run",
      "params": {
        "venv": "venv",
        "path": "app",
        "chain": true,
        "input": null,
        "message": [
          "conda install -y m2w64-toolchain",
          "uv pip install -r requirements.txt",
          "pip install hyperpyyaml modelscope inflect omegaconf conformer diffusers hydra-core lightning x-transformers onnxruntime gdown pyworld matplotlib pyarrow pydantic rich tensorboard protobuf soundfile uvicorn fastapi fastapi-cli grpcio grpcio-tools networkx python-multipart requests"
        ]
      }
    },
    {
      "method": "script.start",
      "params": {
        "uri": "torch.js",
        "params": {
          "path": "app",
          "venv": "venv",
          "xformers": false,
          "triton": false,
          "sageattention": false
        }
      }
    }
  ]
}